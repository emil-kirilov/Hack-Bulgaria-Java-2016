package week7;

import java.util.ArrayList;
import java.util.Scanner;

public class FrogSkippingGame {
	private ArrayList<Character> frogs =  new ArrayList<Character>();
	int leadingFrogLeft;
	int leadingFrogRight;
	int noFrog;
	int[] steps = new int[1000];
	
	public FrogSkippingGame(int frogCount) {
		int noFrog = frogCount; 
		leadingFrogLeft = frogCount - 1;
		leadingFrogRight = frogCount + 1;
		
		
		for (int i = 0; i < frogCount; i++) {
			frogs.add('>');
		}
		frogs.add('_');
		for (int i = 0; i < frogCount; i++) {
			frogs.add('<');
		}
		
	}
	
	public void steps() {
		int counter = 0;
		int sign = 1;
		for (int i = 0; i < frogs.size() / 2; i++) {
			sign *= -1; 
			
			for (int j = 0; j < i; j++) {
				steps[counter] = 2 * sign;
				counter++;
			}
			steps[counter] = sign;
			counter++;
		}
		
		sign *= -1; 
		for (int j = 0; j < frogs.size() / 2; j++) {
			steps[counter] = 2 * sign;
			counter++;
		}
		
		for (int i = 0; i < frogs.size() / 2; i++) {
			sign *= -1;
			
			steps[counter] = sign;
			counter++;
			
			for (int j = frogs.size() / 2 - 1; j > i; j--) {
				steps[counter] = 2 * sign;
				counter++;
			}
		}
		
		
	}
	
	public void test() {
		int sign = -1;
		
		for (int i = 0; i < steps.length; i++) {
			move(noFrog,steps[i]);
		}
	}
	
	public void print(){
		for (int i = 0; i < 6 * frogs.size() / 2; i++) {
			System.out.println(steps[i]);
		}
	}
	
	private void move(int noFrog,int step) {
		char orientation = frogs.get(noFrog + step);
		frogs.set(noFrog, orientation);
		frogs.set(noFrog + step, '_');
		noFrog += step;
		printStrategy();
	}
	
	public void calculateStrategy() {
		skip(leadingFrogLeft);
		printStrategy();
		skip(leadingFrogRight);
		printStrategy();
	}
	
	private void skip(int frog) {
		int step;
		if(frogs.get(frog).equals('>')) {
			step = 1;
		} else {
			step = -1;
		}
		
		if( frogs.get(frog + step).equals('_') ) {
			swap(frog, frog + step);
		} else if( frogs.get(frog + 2 * step).equals('_') ) {
			swap(frog, frog + 2 * step);
		} 
	}
	
	private void swap(int from, int to) {
		char orientation = frogs.get(from);
		frogs.set(from, '_');
		frogs.set(to, orientation);
	}
	
	public void printStrategy() {
		for (int i = 0; i < frogs.size(); i++) {
			System.out.print(frogs.get(i));
		}
		System.out.println();
	}
	
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int numberOfFrogs = s.nextInt();
		FrogSkippingGame fsg = new FrogSkippingGame(numberOfFrogs);
		//fsg.printStrategy();
		fsg.steps();
		fsg.print();
		fsg.test();
		//fsg.calculateStrategy();
		
		s.close();
	}
}
