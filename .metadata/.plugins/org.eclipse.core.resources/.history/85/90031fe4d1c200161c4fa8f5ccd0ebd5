package week9;

public class SkippingFrogsR {
	public void solve() {
		char[]start = {'>', '>', '>', '_', '<', '<', '<'};
		int rock = 3;
		String path = "";
		
		findSolution(start, path, rock);
		System.out.println(path);
	}
	
	private void findSolution(char[] start, String path, int rock) {
		if (start.equals("<<<_>>>")) {
			path += new String(start) + "\n";
			return;
		}
		
		if (start[rock - 1] == '>') {
			start[rock - 1] = '_';
			start[rock] = '>';
			rock = rock - 1;
			
			path += new String(start) + "\n";
			findSolution(start, path, rock);
		}
		
		if (start[rock - 2] == '>') {
			start[rock - 2] = '_';
			start[rock] = '>';
			rock = rock - 2;
			
			path += new String(start) + "\n";
			findSolution(start, path, rock);
		}
		
		if (start[rock + 1] == '<') {
			start[rock + 1] = '_';
			start[rock] = '<';
			rock = rock + 1;
			
			path += new String(start) + "\n";
			findSolution(start, path, rock);
		}
		
		if (start[rock + 2] == '<') {
			start[rock + 2] = '_';
			start[rock] = '<';
			rock = rock + 2;
			
			path += new String(start) + "\n";
			findSolution(start, path, rock);
		}
	}
	
	public static void main(String[] args) {
		solve();
	}
}
