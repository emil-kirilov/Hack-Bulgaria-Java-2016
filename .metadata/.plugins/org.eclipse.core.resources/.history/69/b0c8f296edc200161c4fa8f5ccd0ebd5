package SQL;

public class Client {
	public int id;
	public String name;
	public int age;

	Client(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	public static void main(String[] args) {
		Client kamen = new Client("Kamen", 22);
		
		MySQLHelper.insertInto("Clients", kamen);
		
	}
}
