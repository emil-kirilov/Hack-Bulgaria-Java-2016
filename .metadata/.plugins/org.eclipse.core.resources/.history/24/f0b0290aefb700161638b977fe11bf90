package week7;

import java.util.ArrayList;

public class PandaGraph {
	private class Wrapper {
		private Panda self;
		private ArrayList<Panda> friends;
	
		private Wrapper(Panda p) {
			self = p;
		}
	}
	
	private ArrayList<Wrapper> vertices;
	
	public void add(Panda p) {
		
		vertices.add()
	}
}
